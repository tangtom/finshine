    <!-- 产品管理界面中ListView中背景色为橙色的button -->
    <style name="productManagerButtonColor">
        <item name="android:background">#FF8C22</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 产品管理界面中ListView中背景色为白色的button -->
    <style name="productManagerButtonNormal">
        <item name="android:background">#FFFFFF</item>
