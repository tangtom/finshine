<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"><shape>
            <stroke android:width="1dp"  android:color="#D7C094" />
            <solid android:color="#D0D2D1" />
            <gradient android:angle="270" android:endColor="#D0D2D1" android:startColor="#D0D2D1" />
        </shape></item>
    <item><shape>
            <solid android:color="#00ffffff" />
             <stroke android:width="1dp"  android:color="#D7C094" />
        </shape></item>
</selector> 